def conv2d(image, kernel):
    h, w =3D image.shape
    kh, kw =3D kernel.shape
    out_h =3D h - kh + 1
    out_w =3D w - kw + 1
    output =3D np.zeros((out_h, out_w))
    for i in range(out_h):
        for j in range(out_w):
            region =3D image[i:i+kh, j:j+kw]
            output[i, j] =3D np.sum(region * kernel)
    return output

def relu(x):
    return np.maximum(0, x)

def max_pooling(x, size=3D2, stride=3D2):
    h, w =3D x.shape
    out_h =3D h // size
    out_w =3D w // size
    output =3D np.zeros((out_h, out_w))
    for i in range(out_h):
        for j in range(out_w):
            region =3D x[i*stride:i*stride+size, j*stride:j*stride+size]
            output[i, j] =3D np.max(region)
    return output

